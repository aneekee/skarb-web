{
  "name": "skarb-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "format:write": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.{ts,tsx}",
    "bundlewatch": "bundlewatch --config bundlewatch.config.js",
    "db:pull:local": "dotenv -e .env.local -- npx prisma db pull",
    "db:migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
    "db:push:local": "dotenv -e .env.local -- npx prisma db push"
  },
  "devDependencies": {
    "@auth/prisma-adapter": "2.10.0",
    "@hookform/resolvers": "4.1.3",
    "@prisma/client": "5.22.0",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-table": "8.21.3",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "@types/node": "20.19.11",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "babel-plugin-explicit-exports-references": "1.0.2",
    "bundlewatch": "0.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "2.1.1",
    "dotenv-cli": "7.4.4",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-react": "7.37.5",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lucide-react": "^0.539.0",
    "next": "16.0.10",
    "next-auth": "^5.0.0-beta.30",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.2.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "prisma": "5.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.62.0",
    "recharts": "^2.15.4",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "1.0.7",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "zod": "3.25.76"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "date-fns": "^4.1.0",
    "react-day-picker": "^9.13.0"
  },
